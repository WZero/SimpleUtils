        </activity>
        <activity android:name=".TViewActivity" />
        <activity android:name=".BitmapActivity" />
        <activity android:name=".PaintsActivity" />
        <activity android:name=".RecyclerActivity" />
        <activity android:name=".EasyScrollActivity" />
        <activity android:name=".SpringBackActivity" />
        <activity android:name=".RefreshActivity" />
    </application>

</manifest>